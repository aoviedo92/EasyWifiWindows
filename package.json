{
  "name": "EasyWifiWindows",
  "version": "1.0.0",
  "author": "Adrian Oviedo <adrian.oviedo.1992@gmail.com>",
  "description": "Controla facilmente la red hospedada de Windows",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build-vue": "node .electron-vue/build.js",
    "build_": "node .electron-vue/build.js && electron-builder --dir",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:dir_": "electron-builder --dir",
    "upd-res-electron": "node .electron-vue/electron.rcedit.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "start-electron": "node .electron-vue/start.electron.js",
    "electron-builder": "electron-builder"
  },
  "build": {
    "productName": "EasyWifiWindows",
    "appId": "org.EasyWifiWindows.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "forceCodeSigning": false,
    "electronDist": "D:/tools/Development/node/electron-v1.8.0-win32-ia32",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "verifyUpdateCodeSignature": false,
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "windows-1252": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.7.2",
    "electron-builder": "^19.10.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vuetify": "^0.13.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
